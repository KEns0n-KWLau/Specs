{
  "name": "EffectiveSwift",
  "version": "0.9.0",
  "summary": "Some useful extension for Swift",
  "homepage": "https://github.com/mikezone/EffectiveSwift",
  "license": "GPL-3.0",
  "authors": {
    "Mike": "82643885@qq.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/mikezone/EffectiveSwift.git",
    "tag": "0.9.0"
  },
  "frameworks": [
    "Foundation",
    "UIKit",
    "CoreGraphics"
  ],
  "requires_arc": true,
  "source_files": [
    "EffectiveSwift/Classes/*.swift",
    "EffectiveSwift/Extension/**/*.swift"
  ],
  "preserve_paths": "modulemapFiles/**/*",
  "libraries": "z",
  "pod_target_xcconfig": {
    "SWIFT_VERSION": "3.0",
    "SWIFT_INCLUDE_PATHS[sdk=iphoneos*]": "$(SRCROOT)/EffectiveSwift/modulemapFiles/iphoneos",
    "SWIFT_INCLUDE_PATHS[sdk=iphonesimulator*]": "$(SRCROOT)/EffectiveSwift/modulemapFiles/iphonesimulator"
  },
  "pushed_with_swift_version": "3.0"
}
